{
  "name": "scmp-z",
  "version": "1.0.0",
  "description": "scmp-z",
  "main": "app.js",
  "scripts": {
    "setenv": "source ../config/.env&&export NODE_NAME=scmp-z&&export SCMPZ_VERSION=`git rev-parse HEAD`",
    "prebuild": "yarn install&&rm -rf build",
    "buildDockerImage": "rm -rf Dockerfile&&ln ../config/deploy/Dockerfile .&&npm run prebuild&&EDITION=docker webpack",
    "build": "webpack",
    "compare": "NODE_ENV=development mocha ./test/compare.js",
    "test": "NODE_ENV=development mocha ./test/index.js",
    "start": "NODE_ENV=production NODE_NAME=scmp-z node server.js",
    "dump_image": "node ./dump_image.js",
    "dockerStartApp": "docker-compose -f ../config/deploy/app.yml up -d scmp-z",
    "dockerStopApp": "docker-compose -f ../config/deploy/app.yml stop scmp-z"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/geekplux/koa2-boilerplate.git"
  },
  "keywords": [
    "koa",
    "koa2",
    "babel",
    "es2015"
  ],
  "author": "GeekPlux",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/geekplux/koa2-boilerplate/issues"
  },
  "homepage": "https://github.com/geekplux/koa2-boilerplate#readme",
  "dependencies": {
    "config": "^1.24.0",
    "kcors": "^2.1.1",
    "koa": "^2.0.0",
    "koa-bodyparser": "^3.2.0",
    "koa-compose": "^3.1.0",
    "koa-router": "^7.0.1",
    "mkdirp": "^0.5.1",
    "moment": "^2.13.0",
    "mysql": "^2.15.0",
    "redis":"^2.8.0",
    "redis-crud-fork": "1.0.0-beta.8",
    "scirichon-acl-checker": "1.0.2-beta.4",
    "scirichon-common": "1.0.3-beta.1",
    "scirichon-response-wrapper": "1.0.0-beta.9",
    "scirichon-token-checker": "1.0.1-beta.5"
  },
  "devDependencies": {
    "webpack-builder-advanced": "1.0.0-beta.2"
  }
}
